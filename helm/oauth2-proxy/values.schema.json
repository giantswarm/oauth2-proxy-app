{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "groupID": {
            "type": "integer"
        },
        "ingress": {
            "type": "object",
            "properties": {
                "certSecretName": {
                    "type": "string"
                },
                "className": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "extraAnnotations": {
                    "type": "object"
                },
                "host": {
                    "type": "string"
                },
                "hosts": {
                    "type": "array"
                },
                "letsencrypt": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "tls": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "oauth2Proxy": {
            "type": "object",
            "properties": {
                "cookie": {
                    "type": "object",
                    "properties": {
                        "expiry": {
                            "type": "string"
                        },
                        "refresh": {
                            "type": "string"
                        }
                    }
                },
                "defaultProvider": {
                    "type": "string"
                },
                "emailDomain": {
                    "type": "string"
                },
                "extraArgs": {
                    "type": "array"
                },
                "extraEnv": {
                    "type": "array"
                },
                "oidc": {
                    "type": "object",
                    "properties": {
                        "issuerURL": {
                            "type": "string"
                        },
                        "loginURL": {
                            "type": "string"
                        },
                        "profileURL": {
                            "type": "string"
                        },
                        "redeemURL": {
                            "type": "string"
                        },
                        "validateURL": {
                            "type": "string"
                        }
                    }
                },
                "providers": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "clientID": {
                                "type": "string"
                            },
                            "clientSecret": {
                                "type": "string"
                            },
                            "cookieSecret": {
                                "type": "string"
                            },
                            "existingSecret": {
                                "type": ["null", "string"]
                            },
                            "extraArgs": {
                                "type": "array"
                            },
                            "id": {
                                "type": "string"
                            },
                            "provider": {
                                "type": "string"
                            }
                        }
                    }
                },
                "setAuthorizationHeader": {
                    "type": "boolean"
                },
                "skipProviderButton": {
                    "type": "boolean"
                }
            }
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
                "seccompProfile": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "port": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                }
            }
        },
        "registry": {
            "type": "object",
            "properties": {
                "domain": {
                    "type": "string"
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "seccompProfile": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "userID": {
            "type": "integer"
        }
    }
}
